    "watch-poll": "npm run watch -- --watch-poll",
    "lint": "eslint --ext .js,.vue resources/js/"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.4.1",
    "@vue/eslint-config-prettier": "^4.0.1",
