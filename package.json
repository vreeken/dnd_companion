    "resolve-url-loader": "^2.3.2",
    "sass": "^1.17.2",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.7",
    "vue-template-compiler": "^2.6.7",
    "watch": "^1.0.2"
  },
  "main": "resources/js/app.js",
